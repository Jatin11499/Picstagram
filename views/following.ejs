<%- include("partials/header") -%>
<%- include("partials/nav") -%>

<div class="container" style="margin-top: 75px;">
    <h3 style="margin: 10px 0px;">People you are following:</h3>
    <% for(let i=0;i<following.length;i++){ %>
        <div class="card">
            <div class="card-body f-card">
                <h4 class="card-text"><a href="/open?uname=<%= username %>&fname=<%= fullname %>&acc=<%= following[i] %>" class="acc-name"><%= following[i] %></a></h4>
            </div>
        </div>
    <% } %>
</div>
<%- include("partials/mob-nav") -%>
</body>
</html>